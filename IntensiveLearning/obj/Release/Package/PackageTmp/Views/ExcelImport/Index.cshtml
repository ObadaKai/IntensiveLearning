
@{
    ViewBag.Title = "Index";
}
<div class="WhiteBlur InfoContainer">
    <h2>Imports </h2>
    <div>
        @using (Html.BeginForm("ImportEmployees", "ExcelImport", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <label>Employees</label>
            <input type="file" name="file" />
            <input type="submit" value="OK" />
        }

    </div>
    <div>

        @using (Html.BeginForm("ImportStudents", "ExcelImport", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <label>Students</label>
            <input type="file" name="file" />
            <input type="submit" value="OK" />
        }
    </div>
    <div>
        @using (Html.BeginForm("ImportBnds", "ExcelImport", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <label>Bnds</label>
            <input type="file" name="file" />
            <input type="submit" value="OK" />
        }
    </div>
</div>